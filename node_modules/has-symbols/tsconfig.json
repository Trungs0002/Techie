{
	"compilerOptions": {
		"target": "ES2021",
		"maxNodeModuleJsDepth": 0,
		"module": "commonjs",
		"lib": ["es2021"],
		"declaration": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node"
	},
	"exclude": [
		"coverage"
	]
}
